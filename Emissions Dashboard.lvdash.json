{
  "datasets": [
    {
      "name": "6cc3ad0b",
      "displayName": "Location Data Emissions",
      "queryLines": [
        "select latitude, \r\n",
        "longitude,\r\n",
        "`GHG emissions mtons CO2e`as Emissions\r\n",
        " from emissions.default.emissions_data_2023"
      ]
    },
    {
      "name": "78738ee0",
      "displayName": "Emissions per Person",
      "queryLines": [
        "select county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "           REPLACE(\r\n",
        "               `GHG emissions mtons CO2e`,\r\n",
        "               ',',\r\n",
        "               ''\r\n",
        "           ) AS DOUBLE\r\n",
        "       ) / NULLIF(\r\n",
        "           CAST(\r\n",
        "               REPLACE(\r\n",
        "                   population,\r\n",
        "                   ',',\r\n",
        "                   ''\r\n",
        "               ) AS DOUBLE\r\n",
        "           ),\r\n",
        "           0\r\n",
        "       ) AS Emissions_per_person\r\n",
        "from emissions.default.emissions_data_2023\r\n",
        "order by emissions_per_person desc"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "7775fa3a",
      "displayName": "Total Emissions per State",
      "queryLines": [
        "select state_abbr,\r\n",
        "       sum(CAST(\r\n",
        "           REPLACE(\r\n",
        "               `GHG emissions mtons CO2e`,\r\n",
        "               ',',\r\n",
        "               ''\r\n",
        "           ) AS DOUBLE )) AS total_emissions\r\n",
        "from emissions_data_2023\r\n",
        "group by state_abbr\r\n",
        "order by total_emissions desc\r\n",
        "limit 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    },
    {
      "name": "6b047444",
      "displayName": "County Emissions",
      "queryLines": [
        "select county_state_name,\r\n",
        "       population,\r\n",
        "       CAST(\r\n",
        "           REPLACE(\r\n",
        "               `GHG emissions mtons CO2e`,\r\n",
        "               ',',\r\n",
        "               '')\r\n",
        "       \tAS DOUBLE) as Total_Emissions \r\n",
        "FROM emissions_data_2023\r\n",
        "ORDER BY Total_Emissions desc\r\n",
        "limit 10"
      ],
      "catalog": "emissions",
      "schema": "default"
    }
  ],
