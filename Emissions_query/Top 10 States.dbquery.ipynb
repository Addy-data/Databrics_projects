   "source": [
    "select state_abbr,\n",
    "       sum(CAST(\n",
    "           REPLACE(\n",
    "               `GHG emissions mtons CO2e`,\n",
    "               ',',\n",
    "               ''\n",
    "           ) AS DOUBLE )) AS total_emissions\n",
    "from emissions_data_2023\n",
    "group by state_abbr\n",
    "order by total_emissions desc\n",
    "limit 10"
   ]
  }
